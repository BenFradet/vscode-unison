{
    "name": "vscode-unison",
    "displayName": "Unison syntax highlighting",
    "description": "Syntax highlighting for the Unison programming language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "unison",
            "aliases": ["Unison", "unison"],
            "extensions": [".u", ".uu"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "unison",
            "scopeName": "source.u",
            "path": "./syntaxes/unison.tmLanguage.json"
        }]
    }
}
